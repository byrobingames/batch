<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<palette>
    
    <block tag="initBatchExtension" spec="Start Batch with KEY: %0 and GCM Sender ID(android only): %1 [c:batch]" code="BatchExtension.initBatch(~,~);" help="GCM Sender ID is only for Android, leave blank for iOS" type="action" color="purple" returns="void">
        <fields>
            <text order="0"></text>
            <text order="1"></text>
        </fields>
    </block>


    <block tag="batchRedeemOffer" spec="%0" code="BatchExtension.getRedeemOffer(~)" help="Returns Redeem offer as text" type="normal" color="purple" returns="text">
        <fields>
            <dropdown order="0">
                <choices>
                    <c text="Feature Reference" code="0" />
                    <c text="Feature Value" code="1" />
                    <c text="Ressource Reference" code="2" />
                    <c text="Ressource Quantity" code="3" />
                </choices>
            </dropdown>
        </fields>
    </block>
        
</palette>
